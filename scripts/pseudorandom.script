local prstr = ""
local pr6str = ""
local pr3str = ""
function print_all(key)
	for i=key, 1000 do
		local news_text = prstr:sub(i*15, i*15+15)
		db.actor:give_game_news(news_text, "ui\\ui_iconsTotal", Frect():set(83,470,83,47), 1000, 5000)
	end
end
function random_binary()
	local key = xr_logic.pstor_retrieve(db.actor, "key", 1)
	local iter = xr_logic.pstor_retrieve(db.actor, "iter", 1)
	local len = 40
	if iter*len+len>10000 then
		iter = 1
	end
	local news_text = prstr:sub(iter*len+key, iter*len+len-1+key)
	db.actor:give_game_news(news_text, "ui\\ui_iconsTotal", Frect():set(83,470,83,47), 1000, 5000)
	for i=1, len do
		local news_text1 = news_text:sub(i, i)
		--local news_text1 = news_text:sub(i*15, i*15+15)
		local ntl="no art"
		if news_text1=="1" then
			ntl = pr6str:sub(iter*i+key, iter*i+key)
		end
		db.actor:give_game_news(i.." is "..news_text:sub(i, i).." is "..ntl, "ui\\ui_iconsTotal", Frect():set(83,470,83,47), 1000, 5000)
	end
	xr_logic.pstor_store(db.actor, "iter", iter+1)
end
function set_key()
	local key = math.random(1,100)
	local news_text = key
	--db.actor:give_game_news(news_text, "ui\\ui_iconsTotal", Frect():set(83,470,83,47), 1000, 5000)
	xr_logic.pstor_store(db.actor, "key", key)
end
if xr_logic.pstor_retrieve(db.actor, "key", 1) == 1 then 
	set_key()
end
function random_lock(sid)
	local key = xr_logic.pstor_retrieve(db.actor, "key", 1) + sid
	local lock_str = string.sub(pr3str, key, key)
	return tonumber(lock_str)
end